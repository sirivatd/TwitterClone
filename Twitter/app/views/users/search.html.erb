<h1>Search!</h1>

<nav class="users-search">
<label for="">Name:
  <input class="name-search" type="text" name="" value="">
</label>
</nav>

<ul>
  <% @users.each do |user| %>
    <li>
      <a href="<%= user_url(user) %>"><%= user.username %></a>
      <%= render 'follows/form', user: user %>
    </li>
  <% end %>
</ul>
